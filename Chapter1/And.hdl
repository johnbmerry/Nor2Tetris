/**
 * And gate: 
 * out = 1 if (a == 1 and b == 1)
 *       0 otherwise
 */

CHIP And {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a,out=o1);
	Not(in=b,out=o2);
	Nor(a=o1,b=o2,out=out);
}
